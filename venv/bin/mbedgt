#!/home/peter/Projects/ytdl/venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'mbed-greentea==1.7.2','console_scripts','mbedgt'
__requires__ = 'mbed-greentea==1.7.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('mbed-greentea==1.7.2', 'console_scripts', 'mbedgt')()
    )
